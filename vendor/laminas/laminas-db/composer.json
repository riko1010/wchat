{
    "name": "laminas/laminas-db",
    "description": " ",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "db"
    ],
    "homepage": "https://laminas.dev",
    "support": {
        "docs": "https://docs.laminas.dev/laminas-db/",
        "issues": "https://github.com/laminas/laminas-db/issues",
        "source": "https://github.com/laminas/laminas-db",
        "rss": "https://github.com/laminas/laminas-db/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.6-dev",
            "dev-develop": "2.7-dev"
        }
    },
    "require": {
        "php": ">=5.5",
        "laminas/laminas-stdlib": "~2.7",
        "laminas/laminas-zendframework-bridge": "^1.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "1.7.*",
        "laminas/laminas-eventmanager": "~2.5",
        "laminas/laminas-hydrator": "~1.0",
        "laminas/laminas-mvc": "~2.5",
        "laminas/laminas-servicemanager": "~2.5",
        "phpunit/phpunit": "~4.0"
    },
    "suggest": {
        "laminas/laminas-eventmanager": "Laminas\\EventManager component",
        "laminas/laminas-hydrator": "Laminas\\Hydrator component for using HydratingResultSets",
        "laminas/laminas-servicemanager": "Laminas\\ServiceManager component"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\Db\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasTest\\Db\\": "test/"
        }
    },
    "replace": {
        "zendframework/zend-db": "self.version"
    }
}
